<!-- ic_launcher_foreground.xml -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Bamboo shoot with two leaves -->
    <group android:translateX="48" android:translateY="108">
        <!-- Stalk -->
        <path
            android:fillColor="#2E7D32"
            android:pathData="M0,0 L12,0 L12,-90 L0,-90 Z" />
        <!-- Segment lines -->
        <path
            android:strokeColor="#1B5E20"
            android:strokeWidth="1.5"
            android:fillAlpha="0"
            android:pathData="M0,-70 L12,-70 M0,-45 L12,-45 M0,-20 L12,-20" />
        <!-- Leaf top-right -->
        <path
            android:fillColor="#66BB6A"
            android:pathData="M12,-90 C20,-95 20,-105 12,-110 C16,-100 12,-95 12,-90 Z" />
        <!-- Leaf left -->
        <path
            android:fillColor="#81C784"
            android:pathData="M0,-60 C-10,-65 -10,-75 0,-80 C-4,-70 -4,-65 0,-60 Z" />
    </group>
</vector>

