<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="300dp"
    android:height="200dp"
    android:viewportWidth="300"
    android:viewportHeight="200">

    <!-- Soft Background Gradient -->
    <group>
        <path
            android:pathData="M0,0 L300,0 L300,200 L0,200 Z">
            <gradient
                android:type="linear"
                android:startX="150" android:startY="0"
                android:endX="150" android:endY="200"
                android:startColor="#E8F5E9"
                android:endColor="#C8E6C9"/>
        </path>
    </group>

    <!-- Bamboo Stalk 1 (slightly bent) -->
    <group
        android:translateX="130" android:translateY="200"
        android:rotation="-4" android:pivotX="0" android:pivotY="0">
        <path
            android:fillColor="#66BB6A"
            android:pathData="M0,0 C2,-140 6,-180 0,-200 L14,-200 C8,-180 12,-140 14,0 Z"/>
        <!-- Highlights & Shadow -->
        <path
            android:strokeColor="#A5D6A7"
            android:strokeWidth="2"
            android:fillAlpha="0"
            android:pathData="M2,-200 C3,-180 4,-140 4,0"/>
        <path
            android:strokeColor="#388E3C"
            android:strokeWidth="2"
            android:fillAlpha="0"
            android:pathData="
                M0,-170 L14,-170
                M0,-130 L14,-130
                M0,-90  L14,-90
                M0,-50  L14,-50"/>
    </group>

    <!-- Bamboo Stalk 2 (straight) -->
    <group android:translateX="160" android:translateY="200">
        <path
            android:fillColor="#43A047"
            android:pathData="M0,0 L16,0 L16,-200 L0,-200 Z"/>
        <path
            android:strokeColor="#C8E6C9"
            android:strokeWidth="1.5"
            android:fillAlpha="0"
            android:pathData="
                M0,-180 L16,-180
                M0,-140 L16,-140
                M0,-100 L16,-100
                M0,-60  L16,-60"/>
    </group>

    <!-- Bamboo Stalk 3 (slight lean) -->
    <group
        android:translateX="190" android:translateY="200"
        android:rotation="6" android:pivotX="0" android:pivotY="0">
        <path
            android:fillColor="#66BB6A"
            android:pathData="M0,0 C-2,-120 -4,-160 0,-180 L12,-180 C8,-160 10,-120 12,0 Z"/>
        <path
            android:strokeColor="#A5D6A7"
            android:strokeWidth="2"
            android:fillAlpha="0"
            android:pathData="M2,-180 C2,-160 2,-120 2,0"/>
        <path
            android:strokeColor="#388E3C"
            android:strokeWidth="2"
            android:fillAlpha="0"
            android:pathData="
                M0,-150 L12,-150
                M0,-110 L12,-110
                M0,-70  L12,-70
                M0,-30  L12,-30"/>
    </group>

    <!-- Leaf Clusters -->
    <!-- Left cluster -->
    <path
        android:fillColor="#A5D6A7"
        android:pathData="M130,120 C100,100 90,80 120,70 C110,100 130,110 130,120 Z"/>
    <path
        android:fillColor="#81C784"
        android:pathData="M125,110 C95,90 85,70 115,60 C105,90 125,100 125,110 Z"/>
    <!-- Right cluster -->
    <path
        android:fillColor="#A5D6A7"
        android:pathData="M220,120 C250,100 260,80 230,70 C240,100 220,110 220,120 Z"/>
    <path
        android:fillColor="#81C784"
        android:pathData="M225,110 C255,90 265,70 235,60 C245,90 225,100 225,110 Z"/>

    <!-- Mid-level leaves on each stalk -->
    <path
        android:fillColor="#AED581"
        android:pathData="M138,100 C118,95 118,75 138,70 Z"/>
    <path
        android:fillColor="#AED581"
        android:pathData="M162,100 C182,95 182,75 162,70 Z"/>
    <path
        android:fillColor="#AED581"
        android:pathData="M188,120 C168,115 168,95 188,90 Z"/>

    <!-- Ground accent -->
    <path
        android:fillColor="#66BB6A"
        android:pathData="M0,200 C50,180 250,180 300,200 Z"/>

</vector>
